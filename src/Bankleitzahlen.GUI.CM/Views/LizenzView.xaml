<Window x:Class="Bankleitzahlen.GUI.CM.Views.LizenzView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Bankleitzahlen.GUI.CM.Views"
        xmlns:cal="clr-namespace:Caliburn.Micro;assembly=Caliburn.Micro.Platform"
        xmlns:vm="clr-namespace:Bankleitzahlen.GUI.CM.ViewModels"
        mc:Ignorable="d"
        d:DataContext="{d:DesignInstance Type=vm:LizenzViewModel, IsDesignTimeCreatable=True}"
        cal:Bind.AtDesignTime="True"
        Title="{Binding Projektname}" Height="300" Width="300">
    <Grid>
        <ScrollViewer>
            <TextBox x:Name="LizenzText" TextWrapping="Wrap" IsReadOnly="True" BorderBrush="{x:Null}" BorderThickness="0" />
        </ScrollViewer>
    </Grid>
</Window>
